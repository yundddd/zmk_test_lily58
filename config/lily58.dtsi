right_encoder: encoder_right {
        compatible = "alps,ec11";
        a-gpios = <&pro_micro 21 (GPIO_ACTIVE_HIGH | GPIO_PULL_UP)>; // change these
        b-gpios = <&pro_micro 4 (GPIO_ACTIVE_HIGH | GPIO_PULL_UP)>;
        resolution = <1>;
    };

&left_encoder { resolution = <1>; }; // might as well throw this in here instead of the keymap

&sensors {
    sensors = <&left_encoder &right_encoder>;
}
